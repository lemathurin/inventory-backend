{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress"]
  },
  "include": [
    "**/*.ts",
    "cypress/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
